        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Crear Documento" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import com.mongodb.client.MongoCollection;

import org.bson.Document;

import java.util.Arrays;
def mongoClient = com.mongodb.client.MongoClients.create(&apos;mongodb://eicas:eicaseicas@127.0.0.15:27017/eicas&apos;);
try {

	MongoCollection&lt;Document&gt; collection = vars.getObject(&quot;collection&quot;);
	
	Document document = new Document(&quot;idMayorista&quot;, 1234567)
		.append(&quot;descripcion&quot;, &quot;tudescripcion&quot;)
		.append(&quot;canal&quot;, &quot;linkbanelco&quot;)

	
	collection.insertOne(document);

	return &quot;Document inserted&quot;;
}
catch (Exception e) {
	SampleResult.setSuccessful(false);
	SampleResult.setResponseCode(&quot;500&quot;);
	SampleResult.setResponseMessage(&quot;Exception: &quot; + e);
}</stringProp>
        </JSR223Sampler>
        <hashTree/>
        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Buscar Documento Creado" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import com.mongodb.client.MongoCollection;
import static com.mongodb.client.model.Filters.*;

import org.bson.Document;
import org.bson.types.ObjectId;
import com.mongodb.MongoCredential;


def mongoClient = com.mongodb.client.MongoClients.create(&quot;mongodb://eicas:eicaseicas@127.0.0.15:27017/eicas&quot;);
try {
	MongoCollection&lt;Document&gt; collection = vars.getObject(&quot;collection&quot;);
	
	Document result = collection.find(eq(&quot;canal&quot;, &quot;linkbanelco&quot;)).first();

	vars.put(&quot;exampleDocumentId&quot;, result.get(&quot;_id&quot;).toString());
	
	return &quot;Document with id=&quot; + result.get(&quot;_id&quot;) + &quot; found&quot;;
}
catch (Exception e) {
	SampleResult.setSuccessful(false);
	SampleResult.setResponseCode(&quot;500&quot;);
	SampleResult.setResponseMessage(&quot;Exception: &quot; + e);
}</stringProp>
        </JSR223Sampler>
        <hashTree/>
        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Actualizar Documento" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import com.mongodb.client.MongoCollection;
import static com.mongodb.client.model.Filters.*;
import static com.mongodb.client.model.Updates.*;

import org.bson.Document;
import org.bson.types.ObjectId;
import com.mongodb.MongoCredential;
def mongoClient = com.mongodb.client.MongoClients.create(&quot;mongodb://eicas:eicaseicas@127.0.0.15:27017/eicas&quot;);
try {
	MongoCollection&lt;Document&gt; collection = vars.getObject(&quot;collection&quot;);
	
	collection.updateOne(
		eq(&quot;_id&quot;, new ObjectId(vars.get(&quot;exampleDocumentId&quot;))),
		combine(set(&quot;descripcion&quot;, &quot;nuevadescripcion&quot;)));

	return &quot;Document with id=&quot; + vars.get(&quot;exampleDocumentId&quot;) + &quot; modified&quot;;
}
catch (Exception e) {
	SampleResult.setSuccessful(false);
	SampleResult.setResponseCode(&quot;500&quot;);
	SampleResult.setResponseMessage(&quot;Exception: &quot; + e);
}</stringProp>
        </JSR223Sampler>
        <hashTree/>
        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223  Borrar un documento" enabled="true">
